#: r-novice-gapminder/_episodes/10-functions.md:220
msgid ""
"~~~\n"
"fahr_to_kelvin <- function(temp) {\n"
"  kelvin <- ((temp - 32) * (5 / 9)) + 273.15\n"
"  return(kelvin)\n"
"}\n"
"~~~"
msgstr ""
"~~~\n"
"fahr_to_kelvin <- function(temp) {\n"
