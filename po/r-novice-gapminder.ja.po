"> >     \n"
"> >     ~~~\n"
"> >     # gapminder[gapminder$year == 2002 | 2007,]\n"
"> >     gapminder[gapminder$year == 2002 | gapminder$year == 2007,]\n"
"> >     gapminder[gapminder$year %in% c(2002, 2007),]\n"
