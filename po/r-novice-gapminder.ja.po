"> > ~~~\n"
"> > {: .language-r}\n"
"> > \n"
"> > \n"
"> > \n"
"> > ~~~\n"
"> > [1] 2\n"
"> > ~~~\n"
"> > {: .output}\n"
"> > \n"
"> > ~~~\n"
"> > xlist[[2]][2]\n"
"> > ~~~\n"
"> > {: .language-r}\n"
"> > \n"
"> > \n"
"> > \n"
"> > ~~~\n"
"> > [1] 2\n"
"> > ~~~\n"
"> > {: .output}\n"
"> > \n"
"> > ~~~\n"
