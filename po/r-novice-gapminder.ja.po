"    group_by(continent,year) %>%\n"
"    summarize(mean_gdpPercap=mean(gdpPercap),\n"
"              sd_gdpPercap=sd(gdpPercap),\n"
"              mean_pop=mean(pop),\n"
"              sd_pop=sd(pop),\n"
"              mean_gdp_billion=mean(gdp_billion),\n"
"              sd_gdp_billion=sd(gdp_billion))\n"
