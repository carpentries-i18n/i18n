"~~~\n"
"ddply(\n"
" .data = calcGDP(gapminder),\n"
" .variables = \"continent\",\n"
" .fun = function(x) mean(x$gdp)\n"
")\n"
"~~~"
msgstr ""
"~~~\n"
"ddply(\n"
" .data = calcGDP(gapminder),\n"
" .variables = \"continent\",\n"
