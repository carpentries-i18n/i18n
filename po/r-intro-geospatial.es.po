#: r-intro-geospatial/_episodes/03-data-structures-part1.md:928
msgid ""
"~~~\n"
"list_example <- list(1, \"a\", TRUE, c(2, 6, 7))\n"
"list_example\n"
"~~~"
msgstr ""
"~~~\n"
"list_example <- list(1, \"a\", TRUE, c(2, 6, 7))\n"
"list_example\n"
"~~~"

# code block
#: r-intro-geospatial/_episodes/03-data-structures-part1.md:936
msgid ""
"~~~\n"
"[[1]]\n"
"[1] 1\n"
"\n"
"[[2]]\n"
"[1] \"a\"\n"
"\n"
"[[3]]\n"
"[1] TRUE\n"
"\n"
"[[4]]\n"
"[1] 2 6 7\n"
"~~~"
msgstr ""
"~~~\n"
"[[1]]\n"
"[1] 1\n"
"\n"
"[[2]]\n"
"[1] \"a\"\n"
"\n"
"[[3]]\n"
"[1] TRUE\n"
"\n"
"[[4]]\n"
"[1] 2 6 7\n"
"~~~"

