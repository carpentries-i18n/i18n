"[1] 1\n"
"\n"
"[[2]]\n"
"[1] \"a\"\n"
"\n"
"[[3]]\n"
"[1] TRUE\n"
"\n"
"[[4]]\n"
"[1] 2 6 7\n"
"~~~"
msgstr ""

#: r-intro-geospatial/_episodes/03-data-structures-part1.md:953
# code block
msgid "~~~\n"
"another_list <- list(title = \"Numbers\", numbers = 1:10, data = TRUE )\n"
